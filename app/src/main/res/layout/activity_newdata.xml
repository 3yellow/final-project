        android:background="#00FDF9F9"
        android:ems="10"
        android:inputType="textPersonName"
        android:onClick="onDay2"
        android:text="Button"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
